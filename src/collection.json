{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "project-map": {
      "description": "Adds the dynamic project map module to an Angular app",
      "factory": "./project-map/index#projectMap"
    },
    "ng-add": {
      "factory": "./ng-add/index",
      "description": "Add schematic",
      "aliases": ["install"]
    },
    "ng-generate": {
      "factory": "./ng-generate/index",
      "description": "Generates the scaffolded routes and components"
    }
  }
}
